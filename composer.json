{
	"name": "wnd/app-stub",
	"description": "",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "~8.4.0",
		"php-di/php-di": "^7.0",
		"symfony/config": "^v6.4.8",
		"symfony/expression-language": "^v6.4.8",
		"symfony/http-foundation": "^v6.4.8",
		"symfony/http-kernel": "^v6.4.8",
		"symfony/routing": "^v6.4.8",
		"zircote/swagger-php": "^5.0"
	},
	"require-dev": {
		"codeception/codeception": "^5.1.2",
		"codeception/module-asserts": "^3.0.0",
		"codeception/module-phpbrowser": "^3.0.1",
		"codeception/module-rest": "^3.3.2",
		"phpunit/phpunit": "^11.2.2",
		"roave/security-advisories": "dev-latest"
	},
	"autoload": {
		"psr-4": {
			"wnd\\appStub\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"wnd\\appStub\\tests\\unit\\": "tests/unit/",
			"wnd\\appStub\\tests\\api\\": "tests/api/"
		}
	},
	"scripts": {
		"build-codecept": "codecept build",
		"test-unit": "phpunit -c tests/phpunit.xml",
		"test-api": "codecept run --env dev",
		"openapi": "openapi  src/handlers -o public/openapi.json"
	}
}
